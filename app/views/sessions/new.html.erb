<h1>ログイン</h1>
<%= form_with(scope: :session,url: sessions_path, local:true) do |f| %>
  <%= f.label :email %>
  <br>
  <%= f.email_field :email %>
  <br>
  <%= f.label :password %>
  <br>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.submit "ログインする" %>
<% end %>
<br>
<%= link_to "記事一覧にもどる",articles_path %>
